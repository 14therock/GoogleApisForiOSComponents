			</RestoreAssemblyResource>
			<RestoreAssemblyResource Include="$(XamarinBuildDownloadDir)$(_GoogleUtilitiesItemsFolder)\Libraries\libGTM_SystemVersion.a">
				<LogicalName>libGTM_SystemVersion.a</LogicalName>
				<AssemblyName>$(_GoogleCoreAssemblyName)</AssemblyName>
			</RestoreAssemblyResource>
			<RestoreAssemblyResource Include="$(XamarinBuildDownloadDir)$(_GoogleUtilitiesItemsFolder)\Libraries\libGTM_UIFont+LineHeight.a">
				<LogicalName>libGTM_UIFont+LineHeight.a</LogicalName>
				<AssemblyName>$(_GoogleCoreAssemblyName)</AssemblyName>
			</RestoreAssemblyResource>
			<RestoreAssemblyResource Include="$(XamarinBuildDownloadDir)$(_GoogleUtilitiesItemsFolder)\Libraries\libGTMStackTrace.a">
				<LogicalName>libGTMStackTrace.a</LogicalName>
