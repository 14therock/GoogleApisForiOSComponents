source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '8.0'
install! 'cocoapods', :integrate_targets => false
use_frameworks!

target 'FirebaseCloudFirestore' do
	pod 'Firebase/Firestore', '5.0.0'
	pod 'gRPC-ProtoRPC', '1.8.0'
	pod 'gRPC', '1.8.0'
	pod 'gRPC-Core', '1.8.0'
	pod 'gRPC-RxLibrary', '1.8.0'
	pod 'BoringSSL', '9.0'
end
