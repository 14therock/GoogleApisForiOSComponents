source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '7.0'
install! 'cocoapods', :integrate_targets => false

target 'FirebaseCore' do
	pod 'FirebaseCore', '3.4.4'
	pod 'GoogleToolboxForMac/NSData+zlib', '2.1.0'
	pod 'GoogleToolboxForMac/NSDictionary+URLArguments', '2.1.0'
	pod 'GoogleToolboxForMac/Logger', '2.1.0'
	pod 'GoogleToolboxForMac/StringEncoding', '2.1.0'
	pod 'GoogleToolboxForMac/URLBuilder', '2.1.0'
	pod 'GTMSessionFetcher/Core', '1.1.7'
end
