			</RestoreAssemblyResource>
			<RestoreAssemblyResource Include="$(XamarinBuildDownloadDir)$(_GoogleUtilitiesItemsFolder)\Libraries\libGTM_NSDictionary+URLArguments.a">
				<LogicalName>libGTM_NSDictionary+URLArguments.a</LogicalName>
